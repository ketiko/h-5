<p id="notice"><%= notice %></p>

<h1>Bank Accounts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bank_accounts.each do |bank_account| %>
      <tr>
        <td><%= bank_account.user.email %></td>
        <td><%= bank_account.total %></td>
        <td><%= link_to 'Show', bank_account %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
